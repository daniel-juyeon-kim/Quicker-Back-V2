    "/user": {
      "post": {
        "tags": ["user"],
        "summary": "사용자 회원 가입",
        "requestBody": {
          "description": "새로운 회원 가입",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "walletAddress": {
                    "type": "string",
                    "description": "사용자 지갑주소"
                  },
                  "name": {
                    "type": "string",
                    "description": "사용자 이름"
                  },
                  "email": {
                    "type": "string",
                    "description": "사용자 이메일",
                    "format": "email"
                  },
                  "contact": {
                    "type": "string",
                    "description": "사용자 연락처"
                  },
                  "birthDate": {
                    "type": "string",
                    "example": "2000/01/01",
                    "description": "10 ~ 99세 까지 가능"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "요청 성공",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response200"
                }
              }
            }
          },
          "400": {
            "description": "요청 실패: 유효성 검사 실패",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response400"
                }
              }
            }
          },
          "409": {
            "description": "중복 데이터 존재",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response409"
                }
              }
            }
          },
          "500": {
            "description": "서버 오류",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response500"
                }
              }
            }
          }
        }
      }
    },
