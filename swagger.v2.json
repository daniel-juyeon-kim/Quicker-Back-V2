    "/order": {
      "post": {
        "tags": ["order"],
        "summary": "주문 생성",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "walletAddress": {
                    "type": "string",
                    "description": "사용자의 지갑주소"
                  },
                  "detail": {
                    "type": "string",
                    "nullable": true,
                    "description": "주문에 대한 세부사항"
                  },
                  "transportation": {
                    "type": "array",
                    "description": "운송수단 walking, bicycle, scooter, bike, car, truck 중 하나 이상을 골라 배열로 설정",
                    "items": {
                      "type": "string",
                      "enum": ["walking", "bicycle", "scooter", "bike", "car", "truck"]
                    },
                    "example": ["walking", "bicycle", "scooter", "bike", "car", "truck"],
                    "minItems": 1
                  },
                  "product": {
                    "type": "object",
                    "properties": {
                      "width": {
                        "type": "number",
                        "description": "가로, 단위: cm"
                      },
                      "length": {
                        "type": "number",
                        "description": "세로, 단위: cm"
                      },
                      "height": {
                        "type": "number",
                        "description": "높이, 단위: cm"
                      },
                      "weight": {
                        "type": "number",
                        "description": "무게, 단위:  g"
                      }
                    },
                    "required": ["width", "length", "height", "weight"]
                  },
                  "destination": {
                    "type": "object",
                    "description": "도착지 좌표",
                    "properties": {
                      "x": {
                        "type": "number",
                        "description": "경도"
                      },
                      "y": {
                        "type": "number",
                        "description": "위도"
                      }
                    },
                    "required": ["x", "y"]
                  },
                  "departure": {
                    "type": "object",
                    "description": "출발지 좌표",
                    "properties": {
                      "x": {
                        "type": "number",
                        "description": "경도"
                      },
                      "y": {
                        "type": "number",
                        "description": "위도"
                      }
                    },
                    "required": ["x", "y"]
                  },
                  "sender": {
                    "type": "object",
                    "description": "물건을 보내는 사람에 대한 정보",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "phone": {
                        "type": "string"
                      }
                    },
                    "required": ["name", "phone"]
                  },
                  "receiver": {
                    "type": "object",
                    "description": "수취인에 대한 정보",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "phone": {
                        "type": "string"
                      }
                    },
                    "required": ["name", "phone"]
                  }
                },
                "required": [
                  "walletAddress",
                  "transportation",
                  "product",
                  "destination",
                  "departure",
                  "sender",
                  "receiver"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "요청 성공",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response200"
                }
              }
            }
          },
          "400": {
            "description": "요청 실패: 유효성 검사 실패",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response400"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 사용자",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response404"
                }
              }
            }
          },
          "500": {
            "description": "서버 오류",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/response500"
                }
              }
            }
          }
        }
      }
    },
